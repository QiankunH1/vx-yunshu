<!--index.wxml-->
<view class="container">
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-active-color="{{indicindicatorActiveColor}}" circular="{{circular}}" indicator-color="{{indicatorColor}}" >
  <block wx:for="{{imgUrls}}" wx:key="{{index}}"> 
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150"/>
    </swiper-item>
  </block>
</swiper>

</view>
